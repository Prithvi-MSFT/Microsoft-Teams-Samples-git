{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
  "manifestVersion": "1.12",
  "version": "1.0.0",
  "id": "app_id",
  "packageName": "com.teams.eventmeeting",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.teams.com/privacy",
    "termsOfUseUrl": "https://www.teams.com/termsofuser"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "EventMeeting",
    "full": "EventMeeting"
  },
  "description": {
    "short": "This example is used to manage event meeting.",
    "full": "This example is used to manage event meeting."
  },
  "accentColor": "#5558AF",
  "configurableTabs": [
    {
      "configurationUrl": "base_url/configure",
      "canUpdateConfiguration": true,
      "scopes": [
        "team",
        "groupchat"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    ".*.ngrok.io"
  ]
}