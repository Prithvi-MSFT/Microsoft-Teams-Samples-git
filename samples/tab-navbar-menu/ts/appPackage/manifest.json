{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
  "manifestVersion": "1.12",
  "version": "1.0.0",
  "id": "{{GuID}}",
  "packageName": "com.microsoft.teams.extension",
  "developer": {
    "name": "Teams App, Inc.",
    "websiteUrl": "https://{{domain-name}}",
    "privacyUrl": "https://{{domain-name}}/privacy",
    "termsOfUseUrl": "https://{{domain-name}}/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "NavBarMenu",
    "full": "NavBarMenu"
  },
  "description": {
    "short": "Short description for includes other app options and information.",
    "full": "Full description of includes other app options and information."
  },
  "accentColor": "#FFFFFF",
  "staticTabs": [
    {
      "entityId": "index",
      "name": "Personal Tab",
      "contentUrl": "https://{{domain-name}}/tab",
      "websiteUrl": "https://{{domain-name}}/tab",
      "scopes": [
        "personal"
      ]
    }
  ],
  "validDomains": [
    "{{domain-name}}"
  ]
}